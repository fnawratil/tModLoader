+		public static ushort SwapEndian(bool swap, ushort data)
+		{
+			if (!swap)
+				return data;
+			return (ushort)(
+				((ushort)((data & 0xFF) << 8)) |
+				((ushort)((data >> 8) & 0xFF))
+			);
+		}
+
+		public static uint SwapEndian(bool swap, uint data)
+		{
+			if (!swap)
+				return data;
+			return
+				((data & 0xFF) << 24) |
+				(((data >> 8) & 0xFF) << 16) |
+				(((data >> 16) & 0xFF) << 8) |
+				((data >> 24) & 0xFF);
+		}
+
+		public static ulong SwapEndian(bool swap, ulong data)
+		{
+			if (!swap)
+				return data;
+			return
+				((data & 0xFF) << 56) |
+				(((data >> 8) & 0xFF) << 48) |
+				(((data >> 16) & 0xFF) << 40) |
+				(((data >> 24) & 0xFF) << 32) |
+				(((data >> 32) & 0xFF) << 24) |
+				(((data >> 40) & 0xFF) << 16) |
+				(((data >> 48) & 0xFF) << 8) |
+				((data >> 56) & 0xFF);
+		}
